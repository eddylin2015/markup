extends ./base.pug  

block content
  div#tableContent
    table#EDUTBL
      tr.even
        td
          div 備選同學
          select#srcList(style="width: 150px;height:500px;" , size=20, multiple)
            each st_ in books
              if st_.id==null
                option(value=st_.stud_ref)= `${st_.curr_seat}:${st_.c_name}::`
        td
          button#btn0 全部
          br
          br
          br
          button#btn1 &nbsp; -&gt; &nbsp;  
          br
          br
          br
          button#btn2 &nbsp; &lt;- &nbsp;
        td
          div 已選課同學
          select#disList(style="width: 150px;height:500px;" , size=20, multiple)
            each st_ in books
              if st_.id
                option(value=st_.stud_ref)= `${st_.curr_seat}:${st_.c_name}:${st_.id}:${st_.GC_Name}`
        td
          div 已登分同學
          select#markList(style="width: 150px;height:500px;",  size=20, multiple)